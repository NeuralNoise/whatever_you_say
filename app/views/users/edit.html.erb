<br>
<center>
<h2>Edit User Profile</h2>

<div class="centerFieldset"
<fieldset style="width: 400px">
<% form_for(@user) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<div align=right>
<%= link_to 'Show', @user %> |
<%= link_to 'Back', users_path %>
</div>
</fieldset>

</center>
</div>